        <mxCell id="JNDuL9MOkMPkkqHxkeb9-2" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;autosize=1;resizable=0;" value="&lt;div style=&quot;color: rgb(228, 228, 228); background-color: rgb(24, 24, 24); font-family: Consolas, &amp;quot;Courier New&amp;quot;, monospace; font-size: 14px; line-height: 19px; white-space: pre;&quot;&gt;&lt;div&gt;&lt;span style=&quot;color: #82d2ce;&quot;&gt;function&lt;/span&gt; &lt;span style=&quot;color: #efb080;font-weight: bold;&quot;&gt;createSystemInfoTool&lt;/span&gt;() {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;font-style: italic;&quot;&gt;// 说明书&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #82d2ce;&quot;&gt;const&lt;/span&gt; &lt;span style=&quot;color: #cccccc;&quot;&gt;declaration&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;=&lt;/span&gt; {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;name&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;get_system_info&quot;&lt;/span&gt;,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;description&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;查询电脑系统信息，包括 CPU、内存、进程占用等&quot;&lt;/span&gt;,&lt;/div&gt;&lt;div&gt;        // &lt;span style=&quot;background-color: rgb(255, 255, 255); color: rgb(51, 51, 51); font-family: &amp;quot;Open Sans&amp;quot;, &amp;quot;Clear Sans&amp;quot;, &amp;quot;Helvetica Neue&amp;quot;, Helvetica, Arial, &amp;quot;Segoe UI Emoji&amp;quot;, &amp;quot;SF Pro&amp;quot;, sans-serif; font-size: 16px; orphans: 1; text-wrap-mode: wrap;&quot;&gt;告诉 AI“调用这个工具时要传什么参数”&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;parametersJson&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;JSON&lt;/span&gt;.&lt;span style=&quot;color: #efb080;&quot;&gt;stringify&lt;/span&gt;({&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;type&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;object&quot;&lt;/span&gt;,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;font-style: italic;&quot;&gt;// 参数&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;properties&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;info_type&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;type&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;string&quot;&lt;/span&gt;,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;enum&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;[&lt;/span&gt;&lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;cpu&quot;&lt;/span&gt;, &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;memory&quot;&lt;/span&gt;, &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;process&quot;&lt;/span&gt;, &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;summary&quot;&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;]&lt;/span&gt;,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;description&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;要查询的信息类型：cpu/memory/process/summary&quot;&lt;/span&gt;,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; },&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; },&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;required&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;[&lt;/span&gt;&lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;info_type&quot;&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;]&lt;/span&gt;,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }),&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;options&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;font-style: italic;&quot;&gt;// 选项: 与 TTS 播报相关&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;soothingMessages&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;[&lt;/span&gt;&lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;正在检查系统信息...&quot;&lt;/span&gt;, &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;让我看看电脑状态。&quot;&lt;/span&gt;, &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;马上为你查询。&quot;&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;]&lt;/span&gt;,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;defaultDirectTTS&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; false, &lt;span style=&quot;font-style: italic;&quot;&gt;// 默认把结果**回传给 AI**，不直接 TTS 播报。&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #aaa0fa;&quot;&gt;defaultInterruptMode&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #ebc88d;&quot;&gt;2&lt;/span&gt;, &lt;span style=&quot;font-style: italic;&quot;&gt;// 默认不打断&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; },&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; };&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;font-style: italic;&quot;&gt;// 真正干活的函数&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #82d2ce;&quot;&gt;const&lt;/span&gt; &lt;span style=&quot;color: #efb080;font-weight: bold;&quot;&gt;handler&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;=&lt;/span&gt; (&lt;span style=&quot;color: #d6d6dd;font-style: italic;&quot;&gt;args&lt;/span&gt;) &lt;span style=&quot;color: #82d2ce;&quot;&gt;=&amp;gt;&lt;/span&gt; {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #82d2ce;&quot;&gt;const&lt;/span&gt; &lt;span style=&quot;color: #cccccc;&quot;&gt;typeRaw&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;=&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;font-style: italic;&quot;&gt;args&lt;/span&gt;?.&lt;span style=&quot;color: #aaa0fa;&quot;&gt;info_type&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;||&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;font-style: italic;&quot;&gt;args&lt;/span&gt;?.&lt;span style=&quot;color: #aaa0fa;&quot;&gt;infoType&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;||&lt;/span&gt; &lt;span style=&quot;color: #e394dc;&quot;&gt;&quot;summary&quot;&lt;/span&gt;;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #82d2ce;&quot;&gt;const&lt;/span&gt; &lt;span style=&quot;color: #cccccc;&quot;&gt;infoType&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;=&lt;/span&gt; &lt;span style=&quot;color: #87c3ff;&quot;&gt;String&lt;/span&gt;(&lt;span style=&quot;color: #cccccc;&quot;&gt;typeRaw&lt;/span&gt;).&lt;span style=&quot;color: #efb080;&quot;&gt;toLowerCase&lt;/span&gt;();&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;font-style: italic;&quot;&gt;// 结果返回值：`{ content, directTTS, interruptMode }` &amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #82d2ce;&quot;&gt;const&lt;/span&gt; &lt;span style=&quot;color: #cccccc;&quot;&gt;result&lt;/span&gt; &lt;span style=&quot;color: #d6d6dd;&quot;&gt;=&lt;/span&gt; { &lt;span style=&quot;color: #aaa0fa;&quot;&gt;directTTS&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; false, &lt;span style=&quot;color: #aaa0fa;&quot;&gt;interruptMode&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt; &lt;span style=&quot;color: #ebc88d;&quot;&gt;2&lt;/span&gt; };&lt;/div&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;/div&gt;&lt;div style=&quot;color: rgb(228, 228, 228); background-color: rgb(24, 24, 24); font-family: Consolas, &amp;quot;Courier New&amp;quot;, monospace; font-size: 14px; line-height: 19px; white-space: pre;&quot;&gt;        // 根据类型执行那对应的回调&lt;br&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style=&quot;color: #82d2ce;&quo