&lt;span style=&quot;color: #e4e4e4;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;color: #aaa0fa;&quot;&gt;soothingMessages&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;[]&lt;/span&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt;,&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;color: #e4e4e4;font-style: italic;&quot;&gt;// 默认不直接播报&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;color: #aaa0fa;&quot;&gt;defaultDirectTTS&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt; false,&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;color: #e4e4e4;font-style: italic;&quot;&gt;// 默认不打断&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span style=&quot;color: #aaa0fa;&quot;&gt;defaultInterruptMode&lt;/span&gt;&lt;span style=&quot;color: #d6d6dd;&quot;&gt;:&lt;/span&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;color: #ebc88d;&quot;&gt;2&lt;/span&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt;,&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; };&lt;/span&gt;&lt;/div&gt;&lt;br&gt;&lt;div&gt;&lt;span style=&quot;color: #e4e4e4;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;/span&gt;&lt;span 